{
  "name": "boilr-module",
  "version": "0.0.10",
  "description": "npm module generator with nice configs",
  "license": "MIT",
  "repository": "jforaker/boilr-module",
  "main": "index.js",
  "author": {
    "name": "Jake Foraker",
    "email": "jakeforaker83@gmail.com",
    "url": "https://github.com/jforaker/boilr-module.git"
  },
  "homepage": "https://github.com/jforaker/boilr-module",
  "engines": {
    "node": ">=6"
  },
  "files": ["dist", "index.js"],
  "scripts": {
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "changelog":
      "node ./scripts/changelog && git add . --all && git commit -m 'changelog_ _'",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "docs": "documentation readme src --section=DOCS",
    "clean": "rimraf dist",
    "prebuild": "npm run docs && npm run clean",
    "build": "babel src -d dist",
    "watch": "npm-watch",
    "patch":
      "npm run changelog && npm version patch && git rebase -i HEAD~2 && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublishOnly": "npm run lint && npm test && npm run build",
    "version": "",
    "postversion": "git push && git push --tags && npm publish"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": ["boilerplate, module, npm module boilerplate"],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.18.0",
    "changelog-generator": "0.0.4",
    "documentation": "^5.3.2",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "jest-cli": "^22.1.4",
    "npm-watch": "^0.3.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.1"
  }
}
